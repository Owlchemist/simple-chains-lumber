<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="BenchBase">
		<defName>Owl_WoodDryingRack</defName>
		<label>wood drying rack</label>
		<description>Green lumber can be stored here where it can air dry. But it is susceptible to bad weather.</description>    
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Things/Building/Production/wood_drying_rack</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(2,2)</drawSize>
			<damageData>
				<rectN>(0.125,0.296875,0.734375,0.421875)</rectN>
				<rectS>(0.125,0.296875,0.734375,0.421875)</rectS>
				<rectE>(0.234375,0.296875,0.5,0.546875)</rectE>
				<rectW>(0.234375,0.296875,0.5,0.546875)</rectW>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
			<shadowData>
				<volume>(0.5, 0.5, 0.5)</volume>
				<offset>(0,0,-0.25)</offset>
			</shadowData>
		</graphicData>
		<uiIconPath>Things/Building/Production/wood_drying_rack_north</uiIconPath>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>RawWoody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>10</costStuffCount>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.3</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<Mass>4</Mass>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToBuild>500</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<building>
			<workSpeedPenaltyOutdoors>false</workSpeedPenaltyOutdoors>
			<workSpeedPenaltyTemperature>false</workSpeedPenaltyTemperature>
		</building>
		<comps>
			<li Class="UniversalFermenter.CompProperties_UniversalFermenter">
				<products>
					<li>
						<thingDef>WoodLog</thingDef>
						<ingredientFilter>
							<thingDefs>
								<li>Owl_GreenLumber</li>
							</thingDefs>
						</ingredientFilter>
						<maxCapacity>75</maxCapacity>
						<baseFermentationDuration>54000</baseFermentationDuration>
						<temperatureIdeal>
							<min>10</min>
							<max>100</max>
						</temperatureIdeal>
						<temperatureSafe>
							<min>-50</min>
							<max>200</max>
						</temperatureSafe>
						<sunRespect>
							<min>0.4</min>
							<max>1.0</max>
						</sunRespect>
						<rainRespect>
							<min>1</min>
							<max>0</max>
						</rainRespect>
						<snowRespect>
							<min>1</min>
							<max>0</max>
						</snowRespect>
						<windRespect>
							<min>0.8</min>
							<max>1.0</max>
						</windRespect>
						<graphSuffix>_full</graphSuffix>
					</li>
				</products>
			</li>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<tickerType>Rare</tickerType>
		<rotatable>false</rotatable>
	</ThingDef>
	
	<ThingDef ParentName="BenchBase">
		<defName>Owl_CarpentersTrestle</defName>
		<label>carpenter's trestle</label>
		<description>A pair of trestles can be used for basic woodworking tasks.</description>    
		<thingClass>Building_WorkTable</thingClass>
		<graphicData>
			<texPath>Things/Building/Production/carpenters_trestle</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
            <drawSize>(3.5,1.5)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Production/carpenters_trestle_north</uiIconPath>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>RawWoody</li>
		</stuffCategories>
		<costStuffCount>20</costStuffCount>
		<costList>
            <Steel>20</Steel>
		</costList>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.2</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<Mass>3</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>2000</WorkToBuild>
			<Flammability>0.75</Flammability>
			<WorkTableWorkSpeedFactor>0.66</WorkTableWorkSpeedFactor>
		</statBases>
		<size>(3,1)</size>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(-1,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<workSpeedPenaltyOutdoors>false</workSpeedPenaltyOutdoors>
			<workSpeedPenaltyTemperature>false</workSpeedPenaltyTemperature>
		</building>
		<comps>
			<li Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					<li>ToolCabinet</li>
				</linkableFacilities>
			</li>
		</comps>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
	</ThingDef>
</Defs>